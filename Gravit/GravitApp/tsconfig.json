{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@screens/*": [
        "src/screens/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@navigation/*": [
        "src/navigation/*"
      ],
      "@constants/*": [
        "src/constants/*"
      ]
    },
    "resolveJsonModule": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.json",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}